                </ul>
              </div>
            </div>
        	</div>
          <% if false %>
            <%= f.select :service_type, Vendor::SERVICE_TYPES_LIST, :include_blank => "Selecione o serviço", :html => {id: "service-selection"} %>
          <% else %>
            <%= f.hidden_field :service_type %>
          <% end %>
          <span id="search_name" >
            <%= f.text_field :name, placeholder: "Nome do fornecedor" %>
          </span>
          <%= hidden_field_tag 'service_menu_submit', "2" %>
          <%= f.submit "Buscar", {name: nil, class: "search"} %>
    <% end %>
</div>