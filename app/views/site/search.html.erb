              </div>
              <div style="width:50%;float:left">
                <ul class="categoryBox">
                  <% if false %>
<!--                   <li class="categoryPopupItem categoryPopupItem9" id="categoryPopupItem9" data-value="Beauty" data-value-port="Beleza & Saúde" onclick="catselect.select('9')"><span>Beleza & Saúde</span></li>
                  <li class="categoryPopupItem categoryPopupItem10" id="categoryPopupItem10" data-value="Jewelry" data-value-port="Alianças & Jóias" onclick="catselect.select('10')"><span>Alianças & Jóias</span></li> -->
                  <% end %>
