<div class="" style="width:300px;">
	<div class="banner-wrapper">
		<%= form_for(@search, html: {class: 'jqtransform', id: "search-blog-form", :target => "_parent" }) do |f| %>
          <div id="categoryTextbox" class="categoryTextbox search-blog"> Selecione o serviço</div>
          <div id="categoryPopup" class="roundedBorder popup-blog">
